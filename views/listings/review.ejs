<% layout("/layouts/clientnav") %>
<body>
    <form method="post" action="/review">
        <div>
            <label for="title" class="form-label">title</label>
            <input name="title" value="<%=res.title%>" class="form-control">
        </div>
        <div>
            <label for="name" class="form-label">name</label>
            <input name="name" class="form-control">
        </div>
        <div>
            <label for="rating" class="form-label">rating(1-5)</label>
            <input name="rating" type="number" class="form-control">
        </div>
        <div>
            <label for="comment" class="form-label">comment</label>
            <input name="comment" class="form-control"> 
        </div>
        <button class="btn btn-dark mt-3">Review</button>
    </form>
</body>
